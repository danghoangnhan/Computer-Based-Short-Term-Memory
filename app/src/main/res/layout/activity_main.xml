<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.MainActivity">

    <Button
        android:id="@+id/button1"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="26dp"
        />

    <Button
        android:id="@+id/button2"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="26dp"
        app:layout_constraintLeft_toRightOf="@id/button1"
        />

    <Button
        android:id="@+id/button3"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="26dp"
        app:layout_constraintLeft_toRightOf="@id/button2"
        />
    <Button
        android:id="@+id/button4"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="152dp"
        app:layout_constraintTop_toBottomOf="@id/button1"
        />

    <Button
        android:id="@+id/button5"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="152dp"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintLeft_toRightOf="@id/button4"
        />

    <Button
        android:id="@+id/button6"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="278dp"
        app:layout_constraintTop_toBottomOf="@id/button3"
        app:layout_constraintLeft_toRightOf="@id/button5"
        />

    <Button
        android:id="@+id/button7"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/code"
        android:text="Button"
        app:layout_constraintTop_toBottomOf="@id/button4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="484dp"
        app:layout_constraintTop_toBottomOf="@id/button5"
        app:layout_constraintLeft_toRightOf="@id/button7"
        />
    <Button
        android:id="@+id/button9"
        android:layout_width="116dp"
        android:layout_height="106dp"
        android:background="@drawable/code"
        android:text="Button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="484dp"
        app:layout_constraintTop_toBottomOf="@id/button6"
        app:layout_constraintLeft_toRightOf="@id/button8"
        />

    <Button
        android:id="@+id/nextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        tools:layout_editor_absoluteX="559dp"
        tools:layout_editor_absoluteY="253dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/replayButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Replace"
        tools:layout_editor_absoluteX="514dp"
        tools:layout_editor_absoluteY="307dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/escButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ESC"
        tools:layout_editor_absoluteX="615dp"
        tools:layout_editor_absoluteY="307dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>